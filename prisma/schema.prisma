generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model EventSchemaV1 {
  id       Int    @id @default(autoincrement())
  event_id String @unique
  event    String
  subEvent String
  schema   Json
}

model MaterializedViewMapping {
  id Int @id @default(autoincrement())
  bot_id String
  org_id String
  tableName String
  created_at DateTime @default(now())
}
